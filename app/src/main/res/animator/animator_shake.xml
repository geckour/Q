<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">

    <objectAnimator
        android:duration="70"
        android:propertyName="rotation"
        android:valueFrom="0"
        android:valueTo="25"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="120"
        android:propertyName="rotation"
        android:valueFrom="25"
        android:valueTo="-25"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="120"
        android:propertyName="rotation"
        android:valueFrom="-25"
        android:valueTo="25"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="120"
        android:propertyName="rotation"
        android:valueFrom="25"
        android:valueTo="-25"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="70"
        android:propertyName="rotation"
        android:valueFrom="-25"
        android:valueTo="0"
        android:valueType="floatType" />
</set>